<div class="container">
  <h2 class="text-center">Users</h2>
  <table class="table">
    <thead>
      <tr>
        <th scope="col">Id</th>
        <th scope="col">Users</th>
      </tr>
    </thead>
    <tbody>
      <% @users.each do |user| %>
        <tr>
          <th scope="row"><%= user.id %></th>
          <td><%= link_to user.name, user_path(user)%></td>
        </tr>
      <% end %>
    </tbody>
  </table>
</div>
